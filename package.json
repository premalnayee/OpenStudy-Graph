{
   "name": "osc-coursehub",
   "version": "1.0.0",
   "description": "GraphQL API for Open Study College to manage distance learning courses and collections.",
   "main": "dist/index.js",
   "scripts": {
      "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
      "build": "tsc",
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:run": "npx typeorm migration:run -d src/data-source.ts"
   },
   "keywords": [
      "graphql",
      "typescript",
      "node",
      "express",
      "apollo-server",
      "typeorm",
      "postgres",
      "jwt",
      "typeorm",
      "osc"
   ],
   "author": "Premal Nayee",
   "license": "MIT",
   "dependencies": {
      "apollo-server-express": "^3.11.1",
      "bcryptjs": "^2.4.3",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "graphql": "^15.3.0",
      "jsonwebtoken": "^9.0.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "type-graphql": "^1.1.1",
      "typeorm": "0.3.20"
   },
   "devDependencies": {
      "@types/node": "^16.11.10",
      "ts-node": "^10.9.1",
      "ts-node-dev": "^2.0.0",
      "typescript": "^4.5.2"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/premalnayee/OpenStudy-Graph.git"
   },
   "bugs": {
      "url": "https://github.com/premalnayee/OpenStudy-Graph/issues"
   },
   "homepage": "https://github.com/premalnayee/OpenStudy-Graph#readme"
}
